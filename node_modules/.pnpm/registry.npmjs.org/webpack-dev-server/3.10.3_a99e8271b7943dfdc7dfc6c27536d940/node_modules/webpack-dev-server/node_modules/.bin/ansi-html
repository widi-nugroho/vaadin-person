#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/ansi-html/0.0.7/node_modules/ansi-html/bin/ansi-html/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/ansi-html/0.0.7/node_modules/ansi-html/bin/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/ansi-html/0.0.7/node_modules/ansi-html/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/ansi-html/0.0.7/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/ansi-html/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules:/home/widi/projects/node_modules:/home/widi/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../ansi-html/0.0.7/node_modules/ansi-html/bin/ansi-html" "$@"
  ret=$?
else 
  NODE_PATH="/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/ansi-html/0.0.7/node_modules/ansi-html/bin/ansi-html/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/ansi-html/0.0.7/node_modules/ansi-html/bin/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/ansi-html/0.0.7/node_modules/ansi-html/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/ansi-html/0.0.7/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/ansi-html/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules:/home/widi/projects/node_modules:/home/widi/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../ansi-html/0.0.7/node_modules/ansi-html/bin/ansi-html" "$@"
  ret=$?
fi
exit $ret
