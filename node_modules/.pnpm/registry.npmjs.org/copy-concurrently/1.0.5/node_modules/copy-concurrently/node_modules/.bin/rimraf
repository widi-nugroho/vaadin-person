#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/rimraf/2.7.1/node_modules/rimraf/bin.js/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/rimraf/2.7.1/node_modules/rimraf/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/rimraf/2.7.1/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/rimraf/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules:/home/widi/projects/node_modules:/home/widi/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../rimraf/2.7.1/node_modules/rimraf/bin.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/rimraf/2.7.1/node_modules/rimraf/bin.js/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/rimraf/2.7.1/node_modules/rimraf/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/rimraf/2.7.1/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/rimraf/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules/.pnpm/node_modules:/home/widi/projects/karibu10-helloworld-application/node_modules:/home/widi/projects/node_modules:/home/widi/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../rimraf/2.7.1/node_modules/rimraf/bin.js" "$@"
  ret=$?
fi
exit $ret
